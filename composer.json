{
    "name": "codewars/kata",
    "description": "My codewars.com kata solutions",
    "autoload": {
        "psr-4": {
            "Codewars\\Kata\\": "kata/src"
        }
    },
    "require-dev": {
        "php": "^8.0",
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "Codewars\\Kata\\Tests\\": "kata/tests"
        }
    },
    "require": {
        "ext-mbstring": "*"
    },
    "scripts": {
        "test": [
            "vendor/phpunit/phpunit/phpunit --colors=always"
        ]
    }
}
